	"scripts": {
		"build": "npm run build:tsc && npm run build:rollup",
		"build:rollup": "rollup --config --environment PRODUCTION",
		"build:tsc": "tsc --project tsconfig.json",
		"dev": "npm-run-all --parallel dev:tsc dev:rollup",
		"dev:rollup": "rollup --config --watch.include 'src/**'",
		"dev:tsc": "tsc --project tsconfig.json -w"
