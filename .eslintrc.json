{
	"root": true,
	"parser": "@typescript-eslint/parser",
	"plugins": ["@typescript-eslint"],
	"extends": ["eslint:recommended", "airbnb-base", "plugin:@typescript-eslint/recommended"],
	"env": {
		"browser": true,
		"node": true
	},
	"rules": {
		"import/newline-after-import": 0,
		"indent": ["error", "tab"],
		"no-console": 0,
		"no-tabs": 0
	}
}
